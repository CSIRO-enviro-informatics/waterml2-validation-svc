<?xml version="1.0" encoding="UTF-8"?>
<assert
    xmlns="http://purl.oclc.org/dsdl/schematron"
    test="om:result/schema-element(gml:MultiPointCoverage) | om:result/schema-element(cv:CV_DiscretePointCoverage) | om:result/@xlink:href">result
    must contain an element in the substitution group headed by gml:MultiPointCoverage
    or cv:CV_DiscretePointCoverage or must carry a link</assert>
